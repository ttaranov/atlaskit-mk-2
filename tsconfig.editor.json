{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.collection",
      "es2015.symbol",
      "es2015.iterable",
      "es2015.promise"
    ],
    "types": ["mocha", "react", "enzyme", "css-color-names"],
    "typeRoots": ["./typings", "./node_modules/@types"]
  },
  "files": [
    "./typings/atlaskit.d.ts",
    "./typings/package.json.d.ts",
    "./typings/json.d.ts"
  ]
}
