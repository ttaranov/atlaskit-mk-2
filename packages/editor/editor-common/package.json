    "Oscar W",
    "Stanislav S"
  ],
  "repository": "https://bitbucket.org/atlassian/atlaskit-mk-2",
  "dependencies": {
    "@atlaskit/emoji": "^35.1.1",
    "@atlaskit/icon": "^12.1.2",
    "@atlaskit/media-card": "^29.1.3",
    "@atlaskit/mention": "^13.1.1",
    "@atlaskit/profilecard": "^3.13.0",
    "@atlaskit/util-shared-styles": "^2.10.3",
    "@types/prosemirror-model": "^1.0.0",
    "@types/prosemirror-view": "^1.0.0",
    "ajv": "^4.11.3",
    "classnames": "^2.2.5",
    "css-color-names": "0.0.4",
