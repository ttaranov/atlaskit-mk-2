  },
  "devDependencies": {
    "@atlaskit/editor-json-transformer": "^3.1.2",
    "@atlaskit/editor-test-helpers": "^4.1.3",
    "@atlaskit/json-schema-generator": "^1.0.1",
    "@atlaskit/media-core": "^19.1.3",
    "@atlaskit/webdriver-runner": "^0.0.3",
    "@types/chai": "^4.0.4",
    "chai": "^4.1.2",
