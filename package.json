  "scripts": {
    "build": "                               run-p build:pkg build:babel && run-p build:typescript:es5 build:typescript:es2015 build:editor-common:json-schema --print-label --continue-on-error && yarn build:media-editor:copy-binaries && yarn build:css-reset && yarn build:reduced-ui-pack && yarn build:icon && yarn build:layer && yarn build:mention:json",
    "build:pkg": "                           bolt workspaces exec --only-fs 'packages/**' -- copy-pkg package.json dist/package.json --only name,version",
    "build:babel": "                         run-p build:babel:cjs build:babel:esm",
    "build:babel:cjs": "                     NODE_ENV=production BABEL_ENV=production:cjs bolt workspaces exec --only-fs 'packages/{elements,utils}/**' -- babel src -d dist/cjs",
    "build:babel:esm": "                     NODE_ENV=production BABEL_ENV=production:esm bolt workspaces exec --only-fs 'packages/{elements,utils}/**' -- babel src -d dist/esm",
    "build:typescript:es5": "                NODE_ENV=production bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --project ./build/es5",
    "build:typescript:es2015": "             NODE_ENV=production bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --project ./build/es2015",
    "delete": "                              yarn run delete:modules && yarn run delete:build",
    "delete:modules": "                      rm -rf node_modules **/node_modules",
    "delete:build": "                        rm -rf packages/*/*/dist website/dist",
    "precommit": "                           lint-staged",
    "test": "                                yarn jest",
    "test:changed": "                        RUN_ONLY=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn jest",
    "test:browser": "                        projector ./projector testBrowser",
    "test:browser:watch": "                  projector ./projector testBrowser --watch",
    "test:browser:browserstack": "           projector ./projector testBrowserCI --browserstack",
    "lint": "                                yarn run lint:eslint && yarn run lint:tslint",
    "lint:eslint": "                         eslint --print-config .eslintrc.json | eslint-config-prettier-check && eslint '**/*.js'",
    "lint:tslint": "                         tslint-config-prettier-check ./tslint.json && tslint -c ./tslint.json 'packages/*/*/+(src|tests|__tests__|examples|docs|example-helpers)/**/*.+(ts|tsx)' --format stylish",
    "typecheck": "                           yarn run typecheck:flow && yarn run typecheck:typescript",
    "typecheck:flow": "                      flow check",
    "typecheck:typescript": "                bolt workspaces exec --only-fs 'packages/fabric/*' -- tsc --noEmit --project ./",
    "website:start": "                       cd website && yarn start",
    "start": "                               yarn run website:start",
    "start:elements": "                      yarn run website:start **/packages/elements/**",
    "start:fabric": "                        yarn run website:start **/packages/fabric/**",
    "start:media": "                         yarn run website:start **/packages/fabric/media-*",
    "start:editor": "                        yarn run website:start '**/packages/fabric/+(code|editor-core|editor-common|renderer)'",
    "changeset": "                           node ./runChangeset",
    "release": "                             projector ./projector release",
    "build:editor-common:json-schema": "     cd packages/fabric/editor-common && yarn run build:json-schema && echo 'Done.'",
    "build:media-editor:copy-binaries": "    cd packages/fabric/media-editor && cp -r src/engine/core/binaries dist/es5/engine/core && cp -r src/engine/core/binaries dist/es2015/engine/core",
    "build:icon": "                          cd packages/elements/icon && yarn build",
    "build:reduced-ui-pack": "               cd packages/css-packs/reduced-ui-pack && yarn build",
    "build:css-reset": "                     cd packages/elements/css-reset && yarn build",
    "build:layer": "                         cd packages/elements/layer && yarn build",
    "build:mention:json": "                  cd packages/fabric/mention && cp -r src/support/json-data dist/es5/support/json-data && cp -r src/support/json-data dist/es2015/support/json-data"
  },
