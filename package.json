  "scripts": {
    "build": "                               run-p build:pkg build:babel build:flowtypes && run-p build:typescript build:editor-common:json-schema --print-label --continue-on-error && yarn build:media-editor:copy-binaries && yarn build:css-reset && yarn build:reduced-ui-pack && yarn build:icon && yarn build:layer",
    "build:pkg": "                           bolt workspaces exec --only-fs 'packages/**' -- copy-pkg package.json dist/package.json --only name,version,sideEffects",
    "build:babel": "                         run-p build:babel:cjs build:babel:esm",
    "build:babel:cjs": "                     NODE_ENV=production BABEL_ENV=production:cjs bolt workspaces exec --parallel --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel)\" -- babel src -d dist/cjs --root-mode upward",
    "build:babel:esm": "                     NODE_ENV=production BABEL_ENV=production:esm bolt workspaces exec --parallel --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel)\" -- babel src -d dist/esm --root-mode upward",
    "build:typescript": "                    run-p build:typescript:es5 build:typescript:es2015",
    "build:typescript:es5": "                NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescript)\" -- tsc --project ./build/es5",
    "build:typescript:es2015": "             NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescript)\" -- tsc --project ./build/es2015",
    "build:flowtypes": "                     run-p build:flowtypes:esm build:flowtypes:cjs",
    "build:flowtypes:esm": "                 bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel flow)\" -- flow-copy-source -v -i '**/__tests__/**' src dist/esm",
    "build:flowtypes:cjs": "                 bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel flow)\" -- flow-copy-source -v -i '**/__tests__/**' src dist/cjs",
    "update:icons": "                        bolt w @atlaskit/icon update && bolt w @atlaskit/icon-object update && bolt w @atlaskit/icon-file-type update && bolt w @atlaskit/reduced-ui-pack update",
    "delete": "                              yarn run delete:modules && yarn run delete:build",
    "delete:modules": "                      bolt ws exec --parallel -- rm -rf node_modules && rm -rf node_modules",
    "delete:build": "                        bolt ws exec --parallel -- rm -rf dist",
    "precommit": "                           lint-staged",
    "test": "                                yarn jest",
    "test:changed": "                        CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn jest",
    "test:browser": "                        projector ./projector testBrowser",
    "test:browser:watch": "                  projector ./projector testBrowser --watch",
    "test:browser:browserstack": "           projector ./projector testBrowserCI --browserstack",
    "test:webdriver": "                      node ./build/webdriver-runner/index.js",
    "test:webdriver:watch": "                WATCH=true node ./build/webdriver-runner/index.js",
    "test:webdriver:watch:chrome": "         HEADLESS=false WATCH=true node ./build/webdriver-runner/index.js",
    "test:webdriver:browserstack": "         TEST_ENV=browserstack node ./build/webdriver-runner/index.js",
    "test:webdriver:browserstack:changed": " CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn test:webdriver:browserstack",
    "test:vr": "                             node ./build/visual-regression/index.js",
    "test:vr:local:watch": "                 WATCH=true RUN_LOCAL_ONLY=true node ./build/visual-regression/index.js",
    "test:vr:changed": "                     CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn test:vr",
    "lint": "                                yarn run lint:eslint && yarn run lint:tslint && yarn run lint:stylelint",
    "lint:stylelint": "                      stylelint '**/*.{js,ts,tsx}'",
    "lint:eslint": "                         eslint --print-config .eslintrc.json | eslint-config-prettier-check && eslint '**/*.js'",
    "lint:tslint": "                         yarn run lint:tslint:sources && yarn run lint:tslint:supporting",
    "lint:tslint:sources": "                 tslint-config-prettier-check ./tslint.sources.json && tslint -c ./tslint.sources.json --format stylish 'packages/*/*/src/**/*.+(ts|tsx)' --exclude '**/__tests__/**'",
    "lint:tslint:supporting": "              tslint-config-prettier-check ./tslint.supporting.json && tslint -c ./tslint.supporting.json --format stylish 'packages/*/*/+(tests|__tests__|examples|docs|example-helpers)/**/*.(ts|tsx)' 'packages/*/*/src/**/__tests__/**/*.+(ts|tsx)'",
    "typecheck": "                           yarn run typecheck:flow && yarn run typecheck:typescript",
    "typecheck:flow": "                      flow check",
    "typecheck:typescript": "                tsc --noEmit --project ./tsconfig.typecheck.json",
    "website:start": "                       cd website && yarn start",
    "start": "                               yarn run website:start",
    "start:bitbucket": "                     yarn run website:start **/packages/bitbucket/**",
    "start:confluence": "                    yarn run website:start **/packages/confluence/**",
    "start:core": "                          yarn run website:start **/packages/core/**",
    "start:css-packs": "                     yarn run website:start **/packages/css-packs/**",
    "start:ecosystem": "                     yarn run website:start **/packages/ecosystem/**",
    "start:editor": "                        yarn run website:start **/packages/editor/**",
    "start:elements": "                      yarn run website:start **/packages/elements/**",
    "start:growth": "                        yarn run website:start **/packages/growth/**",
    "start:home": "                          yarn run website:start **/packages/home/**",
    "start:identity": "                      yarn run website:start **/packages/identity/**",
    "start:jira": "                          yarn run website:start **/packages/jira/**",
    "start:marketplace": "                   yarn run website:start **/packages/marketplace/**",
    "start:media": "                         yarn run website:start **/packages/media/**",
    "start:people-and-teams": "              yarn run website:start **/packages/people-and-teams/**",
    "start:pipelines": "                     yarn run website:start **/packages/pipelines/**",
    "start:purchasing": "                    yarn run website:start **/packages/purchasing/**",
    "start:search": "                        yarn run website:start **/packages/search/**",
    "start:service-desk": "                  yarn run website:start **/packages/service-desk/**",
    "start:statuspage": "                    yarn run website:start **/packages/statuspage/**",
    "start:stride": "                        yarn run website:start **/packages/stride/**",
    "start:trello": "                        yarn run website:start **/packages/trello/**",
    "start:teamwork-ecosystem": "            yarn run website:start **/packages/teamwork-ecosystem/**",
    "changeset": "                           node ./build/releases/bin/build-releases.js changeset",
    "build:editor-common:json-schema": "     cd packages/editor/editor-common && yarn run build:json-schema && echo 'Done.'",
    "build:editor-mobile-bridge": "          cd packages/editor/editor-mobile-bridge && yarn build:minimize",
    "build:media-editor:copy-binaries": "    cd packages/media/media-editor && cp -r src/engine/core/binaries dist/es5/engine/core && cp -r src/engine/core/binaries dist/es2015/engine/core",
    "build:icon": "                          cd packages/core/icon && yarn build",
    "build:layer": "                         cd packages/core/layer && yarn build",
    "build:bundle-analysis": "               node ./build/ci-scripts/run.bundle.analysis",
    "build:css-reset": "                     BABEL_ENV=production:cjs bolt w @atlaskit/css-reset run build",
    "build:reduced-ui-pack": "               BABEL_ENV=production:cjs bolt w @atlaskit/reduced-ui-pack run build"
  },
