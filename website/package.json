  "scripts": {
    "build": "yarn build:webpack && yarn build:copy && yarn build:sandbox && yarn build:minimize && yarn build:zip",
    "build:no-minimize": "yarn build:webpack && yarn build:copy && yarn build:zip",
    "build:webpack": "rm -rf dist && WEBSITE_ENV=production webpack ",
    "build:copy": "cp dist/index.html dist/404.html",
    "build:sandbox": "node ./build/sandbox",
    "build:zip": "rm -rf dist.zip && zip -r dist.zip dist/",
    "build:minimize": "find dist -name 'vendor.js' -type f -prune -exec sh -c  \"echo Minimizing: {} && ./node_modules/.bin/uglifyjs --compress hoist_funs=false '{}' -o '{}'\" \\;",
    "start": "webpack-dev-server",
    "deploy": "yarn build && netlify -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID -p dist.zip",
    "deploy-staging": "yarn build && netlify -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip",
    "deploy-staging:no-minimize": "yarn build:no-minimize && netlify -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip"
  },
  "repository": "atlaskit/fabric",
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bolt-query": "^1.1.4",
