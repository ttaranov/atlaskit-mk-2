  "scripts": {
    "webpack:dev": "./node_modules/@atlaskit/webpack-config/bin/dev.js ./src/index.js",
    "webpack:dev:examples": "./node_modules/@atlaskit/webpack-config/bin/dev-examples.js ./src/examples-entry.js",
    "webpack:build": "./node_modules/@atlaskit/webpack-config/bin/build.js ./src/index.js",
    "webpack:report": "./node_modules/@atlaskit/webpack-config/bin/build.js ./src/index.js --report",
    "webpack:build:examples": "./node_modules/@atlaskit/webpack-config/bin/build.js ./src/examples-entry.js",
    "build": "yarn build:webpack && yarn build:copy && yarn build:zip",
    "build:no-minimize": "yarn build:webpack --no-minimize && yarn build:copy && yarn build:zip",
    "build:webpack": "rm -rf dist && yarn run webpack:build",
    "build:copy": "cp dist/index.html dist/404.html && cp -R public/* dist/",
    "build:zip": "rm -rf dist.zip && zip -r dist.zip dist/",
    "start": "yarn webpack:dev",
    "deploy": "WEBSITE_ENV=production yarn build && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID -p dist.zip",
    "deploy-staging": "WEBSITE_ENV=production yarn build && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip",
    "deploy-staging:no-minimize": "WEBSITE_ENV=staging yarn build:no-minimize && netlify-patched -t $NETLIFY_TOKEN deploy --site-id $NETLIFY_SITE_ID_STAGING -p dist.zip"
  },
  "repository": "atlaskit/fabric",
